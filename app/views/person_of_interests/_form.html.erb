<%= form_for @person_of_interest do |f| %>
<br>
    <%= f.label "name" %>
    <%= f.text_field :name %>

    <br>
    <%= f.label "email" %>
    <%= f.text_field :email %>
    <br>
    <%= f.label "phone_number" %>
    <%= f.text_field :phone_number %>
    <br>
    <%= f.label "occupation" %>
    <%= f.text_field :occupation %>
    <br>
    <%= f.label "goals"%>
    <%= f.text_field :goals %>
    <br>
        <%= flash[:errors] if flash[:errors] %>
    <%= f.submit %>
<% end %>
